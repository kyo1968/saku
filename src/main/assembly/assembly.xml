<?xml version="1.0" encoding="UTF-8"?>
<assembly>
	
	<formats>
		<format>zip</format>
	</formats>

	<fileSets>
		<fileSet>
			<directory>conf</directory>
			<outputDirectory>/conf</outputDirectory>
			<includes>
				<include>**/*</include>
			</includes>
		</fileSet>
</fileSets>

	<dependencySets>
		<dependencySet>
			<unpack>false</unpack>
			<scope>compile</scope>
			<outputDirectory>/lib</outputDirectory>
		</dependencySet>
	</dependencySets>
</assembly>
